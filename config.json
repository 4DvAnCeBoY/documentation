{
    "index_name": "lambdatest",
    "start_urls": [
      "https://www.lambdatest.com/support/"
    ],
    "sitemap_urls": [
      "https://www.lambdatest.com/support/sitemap.xml"
    ],
    "sitemap_alternate_links": true,
    "stop_urls": [
      "/tests"
    ],
    "selectors": {
      "lvl0": "article h1, header h1",
      "lvl1": "article h2",
      "lvl2": "article h3",
      "lvl3": "article h4",
      "lvl4": "article h5",
      "text": "article p, article li, article td:last-child"
    },
    "strip_chars": " .,;:#",
    "custom_settings": {
      "separatorsToIndex": "_",
      "attributesForFaceting": [
        "language",
        "version",
        "type",
        "docusaurus_tag"
      ],
      "attributesToRetrieve": [
        "hierarchy",
        "content",
        "anchor",
        "url",
        "url_without_anchor",
        "type"
      ]
    }
  }